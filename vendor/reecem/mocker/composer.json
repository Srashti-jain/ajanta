{
    "name": "reecem/mocker",
    "description": "Mock class constructor args based on the reflection class",
    "require": {
        "illuminate/support": "^5.6|^6.0|^7.0|^8.0",
        "illuminate/filesystem": "^5.6|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^8.0",
        "orchestra/testbench": "^3.5|^4.0"
    },
    "keywords": [
        "laravel", "mockery", "helper", "testing", "mocker", "reecem"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "ReeceM",
            "email": "zsh.rce@gmail.com",
            "homepage": "https://githib.com/reecem"
        }
    ],
    "homepage": "https://github.com/reecem/mocker",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "ReeceM\\Mocker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "style": "phpcs --standard=PSR2 --extensions=php --colors src"
    }
}
