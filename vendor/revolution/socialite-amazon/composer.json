{
  "name": "revolution/socialite-amazon",
  "description": "Socialite for Login with Amazon",
  "keywords": [
    "amazon",
    "socialite",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.2||^8.0",
    "ext-json": "*",
    "laravel/socialite": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0||^9.0",
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^4.0||^5.0"
  },
  "autoload": {
    "psr-4": {
      "Revolution\\Socialite\\Amazon\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Revolution\\Socialite\\Amazon\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "kawax",
      "email": "kawaxbiz@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Revolution\\Socialite\\Amazon\\AmazonServiceProvider"
      ]
    }
  }
}
